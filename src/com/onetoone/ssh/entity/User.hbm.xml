<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.onetoone.ssh.entity">  
    <class name="User" table="user">  
        <id name="id" column="id">  
            <generator class="native"></generator>  
        </id>  
        <property name="username" column="username" type="java.lang.String"  
            not-null="true" length="20"></property>  
        <property name="password" column="password" type="java.lang.String"  
            not-null="true" length="16" />  
        <property name="photo" column="photo" type="java.lang.String"/>  
        <property name="jianjie" column="jianjie" type="java.lang.String"/>
        <property name="fnum" column="fnum" type="java.lang.Integer"/>
        <property name="status" column="status" type="java.lang.Integer"/> 
    </class>  
</hibernate-mapping>