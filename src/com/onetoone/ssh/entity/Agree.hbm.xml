<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.onetoone.ssh.entity">  
    <class name="Agree" table="agree">  
        <id name="id" column="id">  
            <generator class="native"></generator>  
        </id>  
        <many-to-one name="postTie" class="PostTie" column="give_agree_to_tieid" />
        <many-to-one name="user" class="User" column="get_agree_from_userid" />
    </class>  
</hibernate-mapping>